{
  "name": "customjs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "npm run build:ssr",
    "build:prod": "APP_ENVIRONMENT=production npm run build",
    "build:start": "ng b --prod && npm start",
    "build:serve:ssr": "npm run build:ssr && npm run serve:ssr",
    "build:serve:ssr:prod": "APP_ENVIRONMENT=production npm run build:ssr && npm run serve:ssr",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --aot --progress=false --configuration=${APP_ENVIRONMENT} && npm run transpile:ssr",
    "cz": "./node_modules/.bin/git-cz",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "e2e": "ng e2e",
    "format": "prettier --write \"./projects/**/*\" \"./projects/**/*\"",
    "lint": "ng lint",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "npm run serve:ssr",
    "test": "ng test --progress=false --watch=false",
    "test:debug": "clear && ng t --progress=false --source-map=false",
    "test:debug:chrome": "clear && ng t --progress=false --browsers=Chrome",
    "test-once": "ng test --progress=false --watch=false",
    "test:watch": "ng test --progress=false",
    "transpile:ssr": "ng run customjs:server:production --bundleDependencies all",
    "serve:ssr": "node dist/server",
    "build:address-input": "ng b @customjs/address-input && npm run build:address-input:i18n",
    "build:address-input:i18n": "tsc projects/customjs/address-input/src/lib/i18n/*.ts --outDir dist/customjs/address-input/i18n",
    "publish:address-input": "npm publish dist/customjs/address-input/ --access=public",
    "build:alert": "ng b @customjs/alert && npm run build:alert:sass",
    "build:alert:sass": "cp -R projects/customjs/alert/sass dist/customjs/alert/sass",
    "publish:alert": "npm publish dist/customjs/alert/ --access=public",
    "build:ask": "ng b @customjs/ask && npm run build:ask:i18n",
    "build:ask:i18n": "tsc projects/customjs/ask/src/lib/i18n/*.ts --outDir dist/customjs/ask/i18n",
    "publish:ask": "npm publish dist/customjs/ask/ --access=public",
    "build:breadcrumb": "ng b @customjs/breadcrumb",
    "publish:breadcrumb": "npm publish dist/customjs/breadcrumb/ --access=public",
    "build:card": "ng b @customjs/card && npm run build:card:sass",
    "build:card:sass": "cp -R projects/customjs/card/sass dist/customjs/card/sass",
    "publish:card": "npm publish dist/customjs/card/ --access=public",
    "build:currency-input": "ng b @customjs/currency-input",
    "publish:currency-input": "npm publish dist/customjs/currency-input/ --access=public",
    "build:fps": "ng b @customjs/fps",
    "publish:fps": "npm publish dist/customjs/fps/ --access=public",
    "build:file": "ng b @customjs/file",
    "publish:file": "npm publish dist/customjs/file/ --access=public",
    "build:generic-http": "ng b @customjs/generic-http",
    "publish:generic-http": "npm publish dist/customjs/generic-http/ --access=public",
    "build:i18n": "ng b @customjs/i18n",
    "publish:i18n": "npm publish dist/customjs/i18n/ --access=public",
    "build:list": "ng b @customjs/list && npm run build:list:i18n",
    "build:list:i18n": "tsc projects/customjs/list/src/lib/i18n/*.ts --outDir dist/customjs/list/i18n",
    "publish:list": "npm publish dist/customjs/list/ --access=public",
    "build:mask": "ng b @customjs/mask",
    "publish:mask": "npm publish dist/customjs/mask/ --access=public",
    "build:note": "ng b @customjs/note && npm run build:note:sass",
    "build:note:sass": "cp -R projects/customjs/note/sass dist/customjs/note/sass",
    "publish:note": "npm publish dist/customjs/note/ --access=public",
    "publish:nunito": "npm publish projects/customjs/nunito/ --access=public",
    "build:paginator": "ng b @customjs/paginator && npm run build:paginator:i18n",
    "build:paginator:i18n": "tsc projects/customjs/paginator/src/lib/i18n/*.ts --outDir dist/customjs/paginator/i18n",
    "publish:paginator": "npm publish dist/customjs/paginator/ --access=public",
    "build:select": "ng b @customjs/select",
    "publish:select": "npm publish dist/customjs/select/ --access=public",
    "build:table": "ng b @customjs/table && npm run build:table:sass",
    "build:table:sass": "cp -R projects/customjs/table/sass dist/customjs/table/sass",
    "publish:table": "npm publish dist/customjs/table/ --access=public",
    "build:smart-layout": "ng b @customjs/smart-layout",
    "publish:smart-layout": "npm publish dist/customjs/smart-layout/ --access=public",
    "build:version": "ng b @customjs/version && npm run build:version:i18n",
    "build:version:i18n": "tsc projects/customjs/version/src/lib/i18n/*.ts --outDir dist/customjs/version/i18n",
    "publish:version": "npm publish dist/customjs/version/ --access=public",
    "build:ws": "ng b @customjs/ws",
    "publish:ws": "npm publish dist/customjs/ws/ --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/platform-server": "~9.1.12",
    "@angular/pwa": "^0.802.2",
    "@angular/router": "~9.1.12",
    "@angular/service-worker": "~9.1.12",
    "@nguniversal/express-engine": "^9.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@types/express-serve-static-core": "^4.17.9",
    "bufferutil": "^4.0.1",
    "express": "^4.15.2",
    "express-ws": "^4.0.0",
    "g": "^2.0.1",
    "material-design-icons": "^3.0.1",
    "ngx-markdown": "^9.1.1",
    "prismjs": "^1.21.0",
    "rxjs": "~6.6.2",
    "tslib": "^1.13.0",
    "utf-8-validate": "^5.0.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.6.2",
    "codelyzer": "^5.2.2",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^9.1.5",
    "prettier": "^1.19.1",
    "protractor": "^5.4.4",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.38.1",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": "13.8.0",
    "npm": "6.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
